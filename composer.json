{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "beberlei/assert": "^2.7",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "2.7.x-dev",
        "lstrojny/functional-php": "^1.2@dev",
        "prooph/event-sourcing": "^5.3",
        "prooph/event-store-symfony-bundle": "^1.0@dev",
        "prooph/pdo-event-store": "^1.6",
        "prooph/service-bus-symfony-bundle": "^0.6.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^4@dev",
        "symfony/orm-pack": "dev-master",
        "symfony/web-server-bundle": "^3.4",
        "symfony/yaml": "^4.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^3.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Football\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Football\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C42TV5WTV9J5MKQGVBRMDN7H",
            "allow-contrib": false
        }
    }
}
